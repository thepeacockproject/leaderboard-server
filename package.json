{
    "name": "backend",
    "version": "8.0.0",
    "main": "build/main",
    "repository": "https://github.com/RDIL/backend.git",
    "author": "Reece Dunham <me@rdil.rocks>",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "rimraf build && rimraf tsconfig.tsbuildinfo && tsc",
        "prettier": "prettier --write \"**/*.{js,ts,tsx,json,md,html,ejs}\"",
        "format": "yarn prettier",
        "start": "node build/main --enable-source-maps",
        "start:dev": "nest start --watch",
        "postinstall": "prisma generate",
        "prisma": "prisma"
    },
    "devDependencies": {
        "@nestjs/cli": "^10.3.2",
        "@types/cookie-parser": "^1.4.7",
        "@types/ejs": "^3.1.5",
        "@types/express": "^4.17.21",
        "@types/jquery": "^3.5.30",
        "@types/jsonwebtoken": "^9.0.6",
        "@types/node": "^20.12.12",
        "prettier": "^3.2.5",
        "rimraf": "^5.0.7",
        "typescript": "^5.4.5"
    },
    "dependencies": {
        "@nestjs/common": "^10.3.8",
        "@nestjs/core": "^10.3.8",
        "@nestjs/platform-express": "^10.3.6",
        "@nestjs/throttler": "^5.1.2",
        "@octokit/rest": "^20.1.1",
        "@prisma/client": "^5.11.0",
        "@t3-oss/env-core": "^0.10.1",
        "argon2": "^0.40.1",
        "axios": "^1.7.2",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "cookie-parser": "^1.4.6",
        "datatables.net": "^2.0.6",
        "dotenv": "^16.4.5",
        "ejs": "^3.1.10",
        "jquery": "^3.7.1",
        "jsonwebtoken": "^9.0.2",
        "prisma": "^5.11.0",
        "reflect-metadata": "^0.2.2",
        "rxjs": "^7.8.1",
        "tslib": "^2.6.2",
        "zod": "^3.23.8"
    },
    "engines": {
        "node": "18.x || 20.x"
    },
    "prettier": {
        "tabWidth": 4,
        "semi": false
    },
    "cacheDirectories": [
        ".yarn/cache"
    ],
    "packageManager": "yarn@4.1.1"
}
